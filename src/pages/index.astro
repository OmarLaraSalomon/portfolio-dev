---
import Layout from '../layouts/Layout.astro';

import SobreMi from '../components/SobreMi.astro'
import Experiencia from '../components/Experiencia.astro';
import SectionContainer from '../components/SectionContainer.astro';
import ExperienciaIcon from '../components/icons/ExperienciaIcon.astro';
import ProyectoIcon from '../components/icons/ProyectoIcon.astro';
import Proyectos from '../components/Proyectos.astro';
---

<Layout
  title="Porfolio | CódigoWiskas - Desarrollador y Programador Web "
  description="Contrata a WeskerMexx para crear tu aplicación web . Desarrollador Web y Creador de Contenido."
  >
  
	<main class="px-16">


<SectionContainer class="py-1 "  id="sobre-mi">

  <SobreMi/>

</SectionContainer>





<SectionContainer id="experiencia"  class="py-4" >
<h2 class="text-3xl text-purple-700 font-semibold  ml-32 flex gap-x-3 mr-12 justify-items-center  md:items-center">
  
  <ExperienciaIcon class="size-7 text-white" />
  Experiencia Laboral
 </h2>
   <Experiencia/>

</SectionContainer>



<SectionContainer id="proyectos" class="py-16">
  <h2 class=" mb-12 text-3xl text-purple-700 font-semibold  ml-32 flex gap-x-3 mr-12 justify-items-center  md:items-center">
  <ProyectoIcon  class="size-7 " />
  Proyectos
 </h2>


<Proyectos/>
 
</SectionContainer>



</main>
</Layout>


  
    



